                    "id": {
                      "type": "string",
                      "example": "integer"
                    },
                    "image": {
                      "type": "string",
                      "example": "link image"
                    },
                    "date": {
                      "type": "string",
                      "example": "1998-11-11T00:00:00.000Z"
