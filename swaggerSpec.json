                    "name": {
                      "type": "string",
                      "example": "micky mouse"
                    },
                    "rating": {
                      "type": "number",
                      "example": 4.5
                    },
                    "genreId": {
                      "type": "integer",
                      "example": 1
                    },
                    "characters": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer",
                            "example": 1
                          },
                          "name": {
                            "type": "string",
                            "example": "aladdin"
                          },
                          "image": {
                            "type": "string",
                            "example": "link image"
                          },
                          "date": {
                            "type": "string",
                            "example": "1998-11-11T00:00:00.000Z"
                          },
                          "age": {
                            "type": "integer",
                            "example": 25
                          },
                          "story": {
                            "type": "string",
                            "example": "When Aladdin is initially introduced, he is an 18 year old man. He never received a formal education, and has only learned by living on the streets of Agrabah. He has to steal food in the local market in order to survive."
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request.",
